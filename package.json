{
    "name": "torrente-streamer",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "build": "tsc",
        "cleanup": "rm -rf dist/",
        "lint:fix": "npm run lint -- --fix",
        "lint": "tslint -c tslint.json 'src/**/*.ts' -t codeFrame",
        "prettier:fix": "prettier --write './{src,tests}/**/*.ts'",
        "prettier:check": "prettier --check './{src,tests}/**/*.ts'",
        "start:watch": "nodemon -e ts -x 'npm run start' -w ./src",
        "start": "node --inspect=5858 -r ts-node/register ./src/server.ts",
        "test": "LOG_LEVEL=off nyc mocha -r ts-node/register --require source-map-support/register tests/**/*.test.ts",
        "precommit:fixme": "./bin/precommit-fixme"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@types/cors": "^2.8.6",
        "@types/express-fileupload": "^1.1.0",
        "@types/parse-torrent": "^5.8.3",
        "@types/webtorrent": "^0.107.1",
        "bottleneck": "^2.19.5",
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "express-fileupload": "^1.1.6",
        "firebase-admin": "^11.4.1",
        "google-auth-library": "^5.5.1",
        "googleapis": "^45.0.0",
        "log4js": "^6.1.0",
        "luxon": "^1.21.2",
        "node-cache": "^5.0.2",
        "node-fetch": "^2.6.0",
        "parse-torrent": "^7.0.1",
        "webtorrent": "^0.107.17"
    },
    "devDependencies": {
        "@types/express": "^4.17.2",
        "@types/mocha": "^5.2.7",
        "@types/node": "^12.12.12",
        "@types/node-fetch": "^2.5.4",
        "@types/sinon": "^7.5.1",
        "husky": "^3.1.0",
        "mocha": "^6.2.2",
        "nodemon": "^2.0.1",
        "nyc": "^14.1.1",
        "prettier": "^1.19.1",
        "sinon": "^7.5.0",
        "source-map-support": "^0.5.16",
        "ts-node": "^8.5.2",
        "tslint": "^5.20.1",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "3.7.2"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint && npm run prettier:check && npm run precommit:fixme"
        }
    },
    "nyc": {
        "extension": [
            ".ts",
            ".tsx"
        ],
        "exclude": [
            "**/*.d.ts",
            "tests/**/*",
            "coverage/**/*"
        ],
        "include": [
            "src/**/*"
        ],
        "reporter": [
            "html",
            "text-summary"
        ],
        "all": true
    }
}
